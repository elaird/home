# https://twiki.cern.ch/twiki/bin/viewauth/CMS/ClusterUsersGuide

Host lxplus* *.cern.ch cmslpc* *.fnal.gov
     GSSAPITrustDns yes
     GSSAPIAuthentication yes
     GSSAPIDelegateCredentials yes

Host brux*
     ServerAliveInterval 5
     ServerAliveCountMax 30

Host l+cmsusr
     ProxyJump lxplus.cern.ch
     HostName cmsusr

Host l+cms904usr
     ProxyJump lxplus.cern.ch
     HostName cms904usr

# Host x2go*
#      ProxyJump cmsusr

# Host cms904x2go*
#      ProxyJump cms904usr

# SOCKS5 proxy
Host *cmsusr*
     DynamicForward 1080

Host *cms904usr*
     DynamicForward 1081

Host *
     Protocol 2
#    ControlMaster auto
#    ControlPath ~/.ssh/master_%l_%h_%p_%r
